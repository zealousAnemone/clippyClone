{
  "manifest_version": 2,
  "name": "Clippy Cat",
  "version": "1.0",
  "description": "Scourge of the internet",
  "options_page": "options.html",
  "icons": {
    "48": "img/icon.png"
  },
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
    "matches": ["https://*/*"],
    "js": ["js/main.js"],
    "css": ["styles/styles.css"]
    }
  ]
}